	"private": true,
	"scripts": {
		"start:azurite": "azurite -L -l ./temp_azurite -d ./temp_azurite/debug.log",
		"_poe_test": "poetry run poe test",
		"python_authenticate": "poetry config http-basic.resilience resilience $RESILIENCE_ARTIFACTS_PASSWORD",
		"python_install": "poetry install",
		"check": "poetry run poe check",
		"fix": "poetry run poe fix",
		"format": "poetry run poe format",
		"build": "poetry build && poetry run poe build_docs",
		"freeze": "poetry freeze-wheel -vvv",
		"test:unit": "poetry run poe test_unit",
		"test:integration": "poetry run poe test_integration",
		"test:smoke": "poetry run poe test_smoke",
		"test:e2e": "./e2e-test.sh",
		"generate:coverage_report": "poetry run poe coverage_report",
		"test": "run-s 'test:*' coverage_report",
		"run:index": "poetry run poe index",
		"run:query": "poetry run poe query",
		"upload": "poetry run poe upload",
		"publish": "run-s build freeze upload"
